{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Description" : "A VPC with three subnets",
  "Resources" : {
    "MyVPC" : {
      "Type" : "AWS::EC2::VPC",
      "Properties" : {
        "CidrBlock" : "10.2.0.0/16",
        "EnableDnsSupport" : "true",
        "EnableDnsHostnames" : "true",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyVPC"
          }
        ]
      }
    },
    "MyPublicSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : "MyPublicRT",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : "10.2.1.0/24",
        "AvailabilityZone" : "ap-south-1a",
        "MapPublicIpOnLaunch" : "true",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyPublicSubnet"
          }
        ]
      }
    },
    "MyPublicIGW" : {
      "Type" : "AWS::EC2::InternetGateway",
      "Properties" : {
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicIGW"
          }
        ]
      }
    },
    "MyPublicIGWAttachment" : {
      "Type" : "AWS::EC2::VPCGatewayAttachment",
      "DependsOn" : "MyPublicIGW",
      "Properties" : {
        "InternetGatewayId" : { "Ref" : "MyPublicIGW" },
        "VpcId" : { "Ref" : "MyVPC" }
      }
    },
    "MyPublicRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicRT"
          }
        ]
      }
    },
    "MyPublicRoute" : {
      "Type" : "AWS::EC2::Route",
      "DependsOn" : "MyPublicIGW",
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPublicRT" },
        "DestinationCidrBlock" : "0.0.0.0/0",
        "GatewayId" : { "Ref" : "MyPublicIGW" }
      }
    },
    "MyPublicRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyPublicRT", "MyPublicSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPublicRT" },
        "SubnetId" : { "Ref" : "MyPublicSubnet" }
      }
    },
    "MyPublicNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicNACL"
          }
        ]
      }
    },
    "MyPublicNACLInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPublicNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "0.0.0.0/0",
          "Icmp" : { "Code" : "-1", "Type" : "-1" },
          "PortRange" : { "From" : "0", "To" : "65535" }
        }
    },
    "MyPublicNACLOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPublicNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "0.0.0.0/0",
          "Icmp" : { "Code" : "-1", "Type" : "-1" },
          "PortRange" : { "From" : "0", "To" : "65535" }
      }
    },
    "MyPublicSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : "MyPublicSubnet",
      "Properties" : {
        "SubnetId" : { "Ref" : "MyPublicSubnet" },
        "NetworkAclId" : { "Ref" : "MyPublicNACL" }
      }
    },
    "MyPrivateSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : "MyPrivateRT",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : "10.2.2.0/24",
        "AvailabilityZone" : "ap-south-1b",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyPrivateSubnet"
          }
        ]
      }
    },
    "MyPrivateRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPrivateRT"
          }
        ]
      }
    },
    "MyPrivateRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyPrivateRT", "MyPrivateSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPrivateRT" },
        "SubnetId" : { "Ref" : "MyPrivateSubnet" }
      }
    },
    "MyPrivateNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPrivateNACL"
          }
        ]
      }
    },
    "MyPrivateNACLPSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.1.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
        }
    },
    "MyPrivateNACLPSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.1.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
      }
    },
    "MyPrivateNACLDSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "200",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.3.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
        }
    },
    "MyPrivateNACLDSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "200",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.3.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
      }
    },
    "MyPrivateSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : "MyPrivateSubnet",
      "Properties" : {
        "SubnetId" : { "Ref" : "MyPrivateSubnet" },
        "NetworkAclId" : { "Ref" : "MyPrivateNACL" }
      }
    },
    "MyDataSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : "MyDataRT",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : "10.2.3.0/24",
        "AvailabilityZone" : "ap-south-1b",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyDataSubnet"
          }
        ]
      }
    },
    "MyDataRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyDataRT"
          }
        ]
      }
    },
    "MyDataRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyDataRT", "MyDataSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyDataRT" },
        "SubnetId" : { "Ref" : "MyDataSubnet" }
      }
    },
    "MyDataNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyDataNACL"
          }
        ]
      }
    },
    "MyDataNACLPriSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyDataNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.2.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
        }
    },
    "MyDataNACLPriSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyDataNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.2.0/24",
          "PortRange" : { "From" : "0", "To" : "65535" }
      }
    },
    "MyDataSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : "MyDataSubnet",
      "Properties" : {
        "SubnetId" : { "Ref" : "MyDataSubnet" },
        "NetworkAclId" : { "Ref" : "MyDataNACL" }
      }
    }
  }
}
