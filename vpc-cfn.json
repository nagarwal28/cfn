{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Description" : "A VPC with three subnets(Public,Private,Data)",
  "Parameters" : {
    "ParamVPCCIDR" : {
      "Type" : "String",
      "AllowedPattern" : "^(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])",
      "ConstraintDescription" : "CIDR block parameter must be in the form x.x.x.x/16-28",
      "Default" : "10.2.0.0/16",
      "Description" : "VPC CIDR selection"
    },
    "ParamPublicCIDR" : {
      "Type" : "String",
      "AllowedPattern": "^(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])",
      "ConstraintDescription": "CIDR block parameter must be in the form x.x.x.x/16-28 and in range of VPC",
      "Default" : "10.2.1.0/24",
      "Description" : "Public Subnet CIDR selection"
    },
    "ParamPublicAZ" : {
      "Type" : "AWS::EC2::AvailabilityZone::Name",
      "AllowedValues" : { "Fn::GetAZs" : { "Ref" : "AWS::Region" } },
      "Description" : "Public Subnet AZ selection"
    },
    "ParamPrivateCIDR" : {
      "Type" : "String",
      "AllowedPattern": "^(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])",
      "ConstraintDescription": "CIDR block parameter must be in the form x.x.x.x/16-28 and in range of VPC",
      "Default" : "10.2.2.0/24",
      "Description" : "Private Subnet CIDR selection"
    },
    "ParamPrivateAZ" : {
      "Type" : "AWS::EC2::AvailabilityZone::Name",
      "AllowedValues" : { "Fn::GetAZs" : { "Ref" : "AWS::Region" } },
      "Description" : "Private Subnet AZ selection"
    },
    "ParamDataCIDR" : {
      "Type" : "String",
      "AllowedPattern": "^(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d|[01]?\\d\\d|2[0-4]\\d|25[0-5])",
      "ConstraintDescription": "CIDR block parameter must be in the form x.x.x.x/16-28 and in range of VPC",
      "Default" : "10.2.3.0/24",
      "Description" : "Data Subnet CIDR selection"
    },
    "ParamDataAZ" : {
      "Type" : "AWS::EC2::AvailabilityZone::Name",
      "AllowedValues" : { "Fn::GetAZs" : { "Ref" : "AWS::Region" } },
      "Description" : "Data Subnet AZ selection"
    }
  },
  "Resources" : {
    "MyVPC" : {
      "Type" : "AWS::EC2::VPC",
      "Properties" : {
        "CidrBlock" : { "Ref" : "ParamVPCCIDR" },
        "EnableDnsSupport" : "true",
        "EnableDnsHostnames" : "true",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyVPC"
          }
        ]
      }
    },
    "MyPublicSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : [ "MyPublicRT", "MyVPC" ],
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : { "Ref" : "ParamPublicCIDR" },
        "AvailabilityZone" : { "Ref" : "ParamPublicAZ" },
        "MapPublicIpOnLaunch" : "true",
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyPublicSubnet"
          }
        ]
      }
    },
    "MyPublicIGW" : {
      "Type" : "AWS::EC2::InternetGateway",
      "Properties" : {
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicIGW"
          }
        ]
      }
    },
    "MyPublicIGWAttachment" : {
      "Type" : "AWS::EC2::VPCGatewayAttachment",
      "DependsOn" : [ "MyPublicIGW", "MyVPC" ],
      "Properties" : {
        "InternetGatewayId" : { "Ref" : "MyPublicIGW" },
        "VpcId" : { "Ref" : "MyVPC" }
      }
    },
    "MyPublicRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicRT"
          }
        ]
      }
    },
    "MyPublicRoute" : {
      "Type" : "AWS::EC2::Route",
      "DependsOn" : [ "MyPublicIGW", "MyPublicIGW" ],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPublicRT" },
        "DestinationCidrBlock" : "0.0.0.0/0",
        "GatewayId" : { "Ref" : "MyPublicIGW" }
      }
    },
    "MyPublicRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyPublicRT", "MyPublicSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPublicRT" },
        "SubnetId" : { "Ref" : "MyPublicSubnet" }
      }
    },
    "MyPublicNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "DependsOn" : "MyVPC",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPublicNACL"
          }
        ]
      }
    },
    "MyPublicNACLInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPublicNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPublicNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "0.0.0.0/0"
        }
    },
    "MyPublicNACLOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPublicNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPublicNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "0.0.0.0/0"
      }
    },
    "MyPublicSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : [ "MyPublicSubnet", "MyPublicNACL" ],
      "Properties" : {
        "SubnetId" : { "Ref" : "MyPublicSubnet" },
        "NetworkAclId" : { "Ref" : "MyPublicNACL" }
      }
    },
    "MyPrivateSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : [ "MyPrivateRT", "MyVPC" ],
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : { "Ref" : "ParamPrivateCIDR" },
        "AvailabilityZone" : { "Ref" : "ParamPrivateAZ" },
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyPrivateSubnet"
          }
        ]
      }
    },
    "MyPrivateRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "DependsOn" : "MyVPC",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPrivateRT"
          }
        ]
      }
    },
    "MyPrivateRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyPrivateRT", "MyPrivateSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyPrivateRT" },
        "SubnetId" : { "Ref" : "MyPrivateSubnet" }
      }
    },
    "MyPrivateNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "DependsOn" : "MyVPC",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyPrivateNACL"
          }
        ]
      }
    },
    "MyPrivateNACLPSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPrivateNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.1.0/24"
        }
    },
    "MyPrivateNACLPSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPrivateNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.1.0/24"
      }
    },
    "MyPrivateNACLDSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPrivateNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "200",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.3.0/24"
        }
    },
    "MyPrivateNACLDSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyPrivateNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyPrivateNACL" },
          "RuleNumber" : "200",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.3.0/24"
      }
    },
    "MyPrivateSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : [ "MyPrivateSubnet", "MyPrivateNACL" ],
      "Properties" : {
        "SubnetId" : { "Ref" : "MyPrivateSubnet" },
        "NetworkAclId" : { "Ref" : "MyPrivateNACL" }
      }
    },
    "MyDataSubnet" : {
      "Type" : "AWS::EC2::Subnet",
      "DependsOn" : [ "MyDataRT", "MyVPC" ],
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "CidrBlock" : { "Ref" : "ParamDataCIDR" },
        "AvailabilityZone" : { "Ref" : "ParamDataAZ" },
        "Tags" : [
          {
              "Key" : "Name",
              "Value" : "MyDataSubnet"
          }
        ]
      }
    },
    "MyDataRT" : {
      "Type" : "AWS::EC2::RouteTable",
      "DependsOn" : "MyVPC",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyDataRT"
          }
        ]
      }
    },
    "MyDataRTAssociation": {
      "Type" : "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn" : [ "MyDataRT", "MyDataSubnet"],
      "Properties" : {
        "RouteTableId" : { "Ref" : "MyDataRT" },
        "SubnetId" : { "Ref" : "MyDataSubnet" }
      }
    },
    "MyDataNACL" : {
      "Type" : "AWS::EC2::NetworkAcl",
      "DependsOn" : "MyVPC",
      "Properties" : {
        "VpcId" : { "Ref" : "MyVPC" },
        "Tags" : [
          {
            "Key" : "Name",
            "Value" : "MyDataNACL"
          }
        ]
      }
    },
    "MyDataNACLPriSInboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "DependsOn" : "MyDataNACL",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyDataNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "false",
          "CidrBlock" : "10.2.2.0/24"
        }
    },
    "MyDataNACLPriSOutboundEntry" : {
      "Type" : "AWS::EC2::NetworkAclEntry",
      "Properties" : {
          "NetworkAclId" : { "Ref" : "MyDataNACL" },
          "RuleNumber" : "100",
          "Protocol" : "-1",
          "RuleAction" : "allow",
          "Egress" : "true",
          "CidrBlock" : "10.2.2.0/24"
      }
    },
    "MyDataSubnetNACLAssociation" :  {
      "Type" : "AWS::EC2::SubnetNetworkAclAssociation",
      "DependsOn" : [ "MyDataSubnet", "MyDataNACL" ],
      "Properties" : {
        "SubnetId" : { "Ref" : "MyDataSubnet" },
        "NetworkAclId" : { "Ref" : "MyDataNACL" }
      }
    }
  }
}
